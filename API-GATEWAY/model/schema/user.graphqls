# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
    id: ID!
    username: String!
    email: String!
    password: String
    devices: [Device]!
    cratedAt: Int
    updatedAt: Int
    discardedAt: Int
}

type Device {
    id: ID!
    deviceId: String!
    accessToken: String!
    cratedAt: Int
    updatedAt: Int
    discardedAt: Int
}

